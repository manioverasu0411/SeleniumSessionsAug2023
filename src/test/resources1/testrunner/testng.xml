<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression Suite" verbose="2" thread-count="3" parallel="tests" >

	<test name="Google Test">
		<parameter name="url" value="https://www.google.com/"/>
		<parameter name="browser" value="chrome"/>

		<classes>
			<class name="mytest.GoogleTest" />
		</classes>
	</test>
	
	<test name="Amazon Test">
	<parameter name="url" value="https://amazon.com"/>
	<parameter name="browser" value="chrome"/>
	
		<classes>
			<class name="mytest.AmazonTest" />
		</classes>
	</test>
	
	<test name="Open cart Test">
		<parameter name="url" value="https://naveenautomationlabs.com/opencart/"/>
		<parameter name="browser" value="edge"/>

		<classes>
			<class name="mytest.OpenCartTest" />
		</classes>
	</test>


</suite>

